#!/bin/bash
# Description: Cleanup chroot stuff. From https://git.launchpad.net/livecd-rootfs/tree/live-build/ubuntu-cpc/hooks.d/chroot/099-cleanup.chroot

rm -rf /var/log/fsck /var/log/journal
rm -rf /var/run/*
rm -f /etc/passwd-
rm -f /etc/shadow-
rm -f /etc/gshadow-
rm -f /etc/group-
rm -f /etc/apt/conf.d/00secure
rm -rf /home/rolling/.cache
rm -rf /root/.cache
